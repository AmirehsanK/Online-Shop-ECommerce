@{
    if (TempData["Error"] != null)
    {
        var message = TempData["Error"];
        <script>

            Swal.fire({
                icon: 'warning',
                title: 'اخطار',
                text: '@message'
            });
        </script>
    }
    else if (TempData["Success"] != null)
    {
        var message = TempData["Success"];
        <script>
            Swal.fire({
                icon: 'success',
                title: 'اعلان',
                text: @message

            });
        </script>
    }
}
@if (TempData["RegisterResult"] != null)
{
    var register = (string)TempData["RegisterResult"]!;
    if (register == "Error")
    {
        <script>

            Swal.fire({
                icon: 'warning',
                title: 'اخطار',
                text: 'در قرایند ثبت نام مشکلی پیش امده'
            });
        </script>
    }
    else if (register == "Success")
    {
        <script>

            Swal.fire({
                icon: 'success',
                title: 'اعلان',
                text: 'ایمیلی حاوی لینک فعال سازی برای شما ارسال شد'
            });
        </script>
    }
    else if (register == "Invalid")
    {
        <script>

            Swal.fire({
                icon: 'warning',
                title: 'اخطار',
                text: 'کاربری با ایمیل وارد شده موجود میباشد'
            });
        </script>
    }
}

@if (TempData["LoginResult"] != null)
{
    var login = (string)TempData["LoginResult"]!;
    if (login == "NotActive")
    {
        <script>

            Swal.fire({
                icon: 'error',
                title: 'اعلان',
                text: 'ایمیل شما فعال نیست:('
            });
        </script>
    }
    else if (login == "Success")
    {
        <script>

            Swal.fire({
                icon: 'success',
                title: 'اعلان',
                text: 'ثبت نام شما با موفقیت انجام شد'
            });
        </script>
    }
    else if (login == "InvalidPassword")
    {
        <script>

            Swal.fire({
                icon: 'error',
                title: 'اعلان',
                text: 'ایمیل یا پسوورد شما اشتباه میباشید'
            });
        </script>
    }
    else if (login == "Error")
    {
        <script>

            Swal.fire({
                icon: 'error',
                title: 'اعلان',
                text: 'ایمیل یا پسوورد شما اشتباه میباشید'

            });
        </script>
    }
}

@if (TempData["Contactresult"] != null)
